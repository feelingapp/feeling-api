{
  "name": "feeling-api",
  "version": "1.0.0",
  "description": "Internal REST API for Feeling",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavsidhu/feeling-api.git"
  },
  "scripts": {
    "develop": "serverless offline start",
    "migrate": "alembic revision -m",
    "upgrade": "alembic upgrade",
    "setup": "python -m src.setup",
    "deploy-development": "serverless deploy",
    "deploy-production": "serverless deploy --env production",
    "start-database": "docker-compose up -d",
    "stop-database": "docker-compose down"
  },
  "devDependencies": {
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline-python": "^3.21.4",
    "serverless-python-requirements": "^4.2.5"
  }
}
